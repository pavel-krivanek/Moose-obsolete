Extension { #name : #Duration }

{ #category : #'*GToolkit-Examples-Extensions' }
Duration class >> gtExampleOneDay [
	<gtExample>
	^ self days: 1
]

{ #category : #'*GToolkit-Examples-Extensions' }
Duration class >> gtExampleOneHour [
	<gtExample>
	^ self hours: 1
]

{ #category : #'*GToolkit-Examples-Extensions' }
Duration class >> gtExampleOneMinute [
	<gtExample>
	^ self minutes: 1
]
